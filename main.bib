  month = {jul},
  number = {151},
  pages = {1003--1026},
  publisher = {American Mathematical Society},
  title = {The Pseudoprimes to $25 \cdot 10^9$},
  url = {http://links.jstor.org/sici?sici=0025-5718%28198007%2935%3A151%3C1003%3ATPT%3E2.0.CO%3B2-D},
